{
  "appId": "com.photoshare.photo-share",
  "appName": "PhotoShare",
  "webDir": "www",
  "server": {
    "url": "https://photo-share.app",
    "cleartext": false
  },
  "plugins": {
    "FirebaseApp": {
      "file": "GoogleService-Info.plist"
    },
    "FirebaseAuthentication": {
      "skipNativeAuth": false,
      "providers": ["google.com"]
    },
    "StatusBar": {
      "style": "DEFAULT",
      "backgroundColor": "#ffffff",
      "overlaysWebView": false
    },
    "FirebaseMessaging": {
      "presentationOptions": ["badge", "sound", "alert"]
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    },
    "Camera": {},
    "EventPhotoPicker": {},
    "UploadManager": {},
    "PhotoLibraryMonitor": {},
    "QRScanner": {},
    "AppPermissionPlugin": {}
  },
  "ios": {
    "scheme": "App",
    "contentInset": "automatic",
    "backgroundColor": "#00000000",
    "handleApplicationNotifications": false,
    "scrollEnabled": true,
    "bounces": true
  },
  "packageClassList": [
    "SignInWithApple",
    "CameraPreview",
    "DatePickerPlugin",
    "PhotoViewerPlugin",
    "FirebaseAppPlugin",
    "FirebaseAuthenticationPlugin",
    "FirebaseMessagingPlugin",
    "AppPlugin",
    "DevicePlugin",
    "DialogPlugin",
    "FilesystemPlugin",
    "HapticsPlugin",
    "KeyboardPlugin",
    "PreferencesPlugin",
    "PushNotificationsPlugin",
    "StatusBarPlugin",
    "EventPhotoPicker",
    "UploadManager",
    "PhotoLibraryMonitor",
    "QRScanner",
    "AppPermissionPlugin"
  ]
}
