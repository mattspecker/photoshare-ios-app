{
  "appId": "com.photoshare.photo-share",
  "appName": "PhotoShare",
  "webDir": "www",
  "server": {
    "url": "https://photoshare.ai",
    "cleartext": false
  },
  "plugins": {
    "FirebaseApp": {
      "file": "GoogleService-Info.plist"
    },
    "FirebaseAuthentication": {
      "skipNativeAuth": false,
      "providers": ["google.com"]
    },
    "Camera": {
      "permissions": ["camera", "photos"]
    },
    "StatusBar": {
      "style": "DEFAULT",
      "backgroundColor": "#ffffff",
      "overlaysWebView": false
    },
    "FirebaseMessaging": {
      "presentationOptions": ["badge", "sound", "alert"]
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    },
    "App": {},
    "Device": {},
    "Dialog": {},
    "Filesystem": {},
    "Haptics": {},
    "Keyboard": {
      "resize": "ionic",
      "resizeOnFullScreen": true,
      "style": "dark"
    },
    "Preferences": {},
    "AppleSignIn": {},
    "DatePicker": {},
    "PhotoViewer": {},
    "FileTransfer": {},
    "EventPhotoPicker": {},
    "UploadManager": {},
    "PhotoLibraryMonitor": {},
    "QRScanner": {},
    "AppPermissionPlugin": {},
    "PhotoEditorPlugin": {},
    "AutoUploadPlugin": {},
    "UploadStatusOverlay": {},
    "CustomCameraPlugin": {},
    "NativeGalleryPlugin": {
      "downloadEnabled": true,
      "shareEnabled": true,
      "customViewer": true
    },
    "BulkDownloadPlugin": {
      "enabled": true
    }
  },
  "ios": {
    "scheme": "App",
    "contentInset": "automatic",
    "backgroundColor": "#ffffff",
    "handleApplicationNotifications": false
  },
  "packageClassList": [
    "SignInWithApple",
    "DatePickerPlugin",
    "PhotoViewerPlugin",
    "FirebaseAppPlugin",
    "FirebaseAuthenticationPlugin",
    "FirebaseMessagingPlugin",
    "AppPlugin",
    "CAPCameraPlugin",
    "DevicePlugin",
    "DialogPlugin",
    "FileTransferPlugin",
    "FilesystemPlugin",
    "HapticsPlugin",
    "KeyboardPlugin",
    "PreferencesPlugin",
    "PushNotificationsPlugin",
    "StatusBarPlugin",
    "EventPhotoPicker",
    "UploadManager",
    "PhotoLibraryMonitor",
    "QRScanner",
    "AppPermissionPlugin",
    "PhotoEditorPlugin",
    "AutoUploadPlugin",
    "UploadStatusOverlay",
    "CustomCameraPlugin",
    "NativeGalleryPlugin",
    "BulkDownloadPlugin",
    "FCMTokenPlugin",
    "DeepLinkRouter",
    "PerformanceMonitorPlugin"
  ]
}
